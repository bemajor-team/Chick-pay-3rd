{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowVeleroAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::297195401389:role/velero-irsa-role"
      },
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::chickpay-backup",
        "arn:aws:s3:::chickpay-backup/*"
      ]
    }
  ]
}
